{
  "name": "biskaken-frontend",
  "type": "application",
  "buildType": "docker",
  "dockerfile": "Dockerfile.frontend",
  "repository": {
    "url": "https://github.com/plunoo/biskakenauto.git",
    "branch": "main",
    "buildPath": "."
  },
  "domain": {
    "host": "biskakenauto.rpnmore.com",
    "ssl": true
  },
  "environment": {
    "NODE_ENV": "production",
    "VITE_API_URL": "https://biskakenauto.rpnmore.com"
  },
  "healthcheck": {
    "enabled": true,
    "path": "/",
    "interval": 30000
  },
  "resources": {
    "memory": "512MB",
    "cpu": "0.25"
  }
}