{
  "name": "biskaken-auto-shop",
  "type": "application",
  "buildType": "docker",
  "dockerfile": "Dockerfile.fullstack-unified",
  "repository": {
    "url": "https://github.com/plunoo/biskakenauto.git",
    "branch": "main",
    "buildPath": "."
  },
  "domain": {
    "host": "biskakenauto.rpnmore.com",
    "ssl": true
  },
  "environment": {
    "NODE_ENV": "production",
    "PORT": "3000",
    "APP_URL": "https://biskakenauto.rpnmore.com",
    "VITE_API_URL": "https://biskakenauto.rpnmore.com",
    "DATABASE_URL": "file:./dev.db",
    "JWT_SECRET": "default-insecure-secret-change-in-production"
  },
  "healthcheck": {
    "enabled": true,
    "path": "/health",
    "interval": 30000
  },
  "resources": {
    "memory": "1024MB",
    "cpu": "0.5"
  }
}