{
  "name": "biskaken-auto-shop",
  "type": "application",
  "buildType": "nixpacks",
  "repository": {
    "url": "https://github.com/plunoo/biskakenauto.git",
    "branch": "main",
    "buildPath": "."
  },
  "domain": {
    "host": "biskakenauto.rpnmore.com",
    "ssl": true
  },
  "environment": {
    "NODE_ENV": "production",
    "PORT": "5000",
    "APP_URL": "https://biskakenauto.rpnmore.com",
    "VITE_API_URL": "https://biskakenauto.rpnmore.com"
  },
  "build": {
    "command": "npm run deploy:build",
    "outputDirectory": "."
  },
  "start": {
    "command": "cd biskaken-auto-api && npm start"
  },
  "healthcheck": {
    "enabled": true,
    "path": "/health",
    "interval": 30000
  },
  "resources": {
    "memory": "1024MB",
    "cpu": "0.5"
  }
}